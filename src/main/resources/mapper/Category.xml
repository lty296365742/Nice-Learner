<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.example.learner.mapper.CategoryMapper">
    <resultMap id="tb_category" type="com.example.learner.bean.Category">
        <id column="id" jdbcType="INTEGER" property="id"/>
        <result column="aspectId" jdbcType="INTEGER" property="aspectId"/>
        <result column="category" jdbcType="VARCHAR" property="category"/>
    </resultMap>
    
</mapper>
